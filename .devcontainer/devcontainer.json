{
  "name": "FleetMan dev",
  "image": "mcr.microsoft.com/devcontainers/dotnet:8.0",
  "postCreateCommand": "dotnet restore",
  "customizations": {
    "vscode": {
      "extensions": [
        "MarwanAlwan.dv",
        "humao.rest-client",
        "Bob0911.namespace-starter"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "zsh",
        "terminal.integrated.profiles.linux": {
          "zsh": {
            "path": "/bin/zsh"
          }
        }
      }
    }
  }
}

